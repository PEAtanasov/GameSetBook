@model ClubInfoViewModel

<div class="club-info text-break">
    <img src="~/@Model.Logourl" alt="@Model.Name Logo" class="img-fluid" style="max-width: auto; max-height: auto;" />
    <p><strong>@Model.Name</strong></p>
    <p><strong>Phone Number:</strong> @Model.PhoneNumber</p>
    <p><strong>Email:</strong> @Model.Email</p>
    <p><strong>Address:</strong> @Model.Address</p>
    <p><strong>City:</strong> @Model.CityName</p>
    <p><strong>Country:</strong> @Model.CountryName</p>
</div>